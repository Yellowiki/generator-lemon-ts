{
  "name": "generator-lemon-ts",
  "version": "0.0.0-development",
  "description": "Generates a TypeScript project",
  "author": {
    "name": "GloriousYellow",
    "email": "yellowglorious@gmail.com"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": ["yeoman-generator"],
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.1.0",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-plugin-prettier": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.0.0",
    "jest-cli": "^21.0.0",
    "lint-staged": "^6.0.0",
    "nsp": "^3.1.0",
    "prettier": "^1.7.0",
    "semantic-release": "^8.2.0",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "add-prettier": "^1.2.0",
    "chalk": "^2.1.0",
    "create-react-app": "^1.4.1",
    "deep-assign": "^2.0.0",
    "fs-extra": "^5.0.0",
    "install-peerdeps": "^1.2.0",
    "semantic-release-cli": "^4.0.6",
    "serve": "^6.3.1",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest",
    "format": "eslint --fix .",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "lint-staged": {
    "*.js": ["eslint --fix", "git add"],
    "*.json": ["prettier --write", "git add"]
  },
  "eslintConfig": {
    "extends": ["xo", "prettier"],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": ["prettier"]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 150
  },
  "repository": "https://github.com/Yellowiki/generator-lemon-ts.git",
  "license": "GPL-3.0"
}
